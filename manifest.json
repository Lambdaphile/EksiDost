{

    "manifest_version": 2,
    "name": "eksiplus",
    "version": "1.0.0",

    "description": "Colors visited entry links on eksisozluk.com for better user experience while browsing topic lists",

    "icons": {
      "48": "icons/EksiPlus_48x48.png",
      "96": "icons/EksiPlus_96x96.png"
    },

    "content_scripts": [
      {
        "matches": ["https://eksisozluk.com/*"],
        "css": ["eksi.css"]
      }
    ]

  }